# white noise
t <- 100
t_i <- seq(1, t, 0.1)
WN <- rnorm(length(t_i), 0, 1)
# path
WN_ns <- sqrt(t_i) * WN * sin(t_i)
# total return
sum(WN_ns / t)
# initial stock price plus returns
stock_ret <- cumsum(WN_ns)
stock <- t + stock_ret
if (sum(stock < 0) > 0) {
print("Bankrupt!")
} else {
plot(t_i, stock, type = 'l')
}
mean(stock)
sd(stock)
